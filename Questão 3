#include<stdio.h>
#include<stdlib.h>

int main () {
	
	float altura;
	
	float base;
	
	printf("qual a altura do retangulo ");
	scanf("%f", &altura);
	
	printf("qual a base do retangulo");
	scanf("%f", &base);
	
	float area = base * altura;
	
	printf("a area do retangulo e %f", area);


	return 0;
	
}
