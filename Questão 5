#include <stdio.h>
#include <stdlib.h>

int main () {
	
	float numero1, numero2;
		
	printf("digite um numero\n");
	scanf("%i", &numero1);
	
	
	printf("digite outro numero\n");
	scanf("%i", &numero2);
	
	
	float divisao = numero2 / numero1;
	
	printf("\na divisao dos numeros e %f", divisao);

	return 0;
	
}
