#include <stdio.h>
#include <stdlib.h>

int main () {
	
	float Fahrenheit;
	
	
		
	printf("digitea temperatura em Fahrenheit ");
	scanf("%f", &Fahrenheit);
	
	float celcius = (Fahrenheit - 32) / 1.8 ;
	
	
	
	 
	
	printf("\na temperatura em graus celcius e %f ", celcius);
	
	printf("\na temperatura em Fahrenheit e %f ", Fahrenheit);

	return 0;
	
}
