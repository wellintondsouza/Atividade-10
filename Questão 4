#include <stdio.h>
#include <stdlib.h>

int main () {
	
	int numero1, numero2;
		
	printf("digite um numero\n");
	scanf("%i", &numero1);
	
	
	printf("digite outro numero\n");
	scanf("%i", &numero2);
	
	
	int diferenca = numero1 - numero2;
	
	printf("\na diferen√ßa dos numeros e %i", diferenca);

	return 0;
	
}
